{
  "name": "seekr",
  "version": "1.4.2",
  "description": "A package for detecting keywords on the Internet Computer",
  "author": "Ryan Cammer <ryancammer@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Internet-Computer-Tools/seekr.git"
  },
  "license": "MIT",
  "keywords": [
    "Internet Computer",
    "crawler",
    "esbuild"
  ],
  "seekr": "./dist/tsc/src/seekr.js",
  "bin": {
    "cli": "dist/tsc/src/cli.js"
  },
  "scripts": {
    "cli": "ts-node src/cli.ts",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "test": "jest",
    "clean": "rm -rf dist build package",
    "ts-node": "ts-node",
    "docs": "typedoc --entryPoints src/main.ts",
    "build": "tsc -p tsconfig.json"
  },
  "dependencies": {
    "async": "^3.2.5",
    "axios": "^0.27.2",
    "axios-request-throttle": "^1.0.0",
    "commander": "^11.1.0",
    "jest": "^29.1.1",
    "native-dns": "^0.7.0",
    "puppeteer": "^18.0.5",
    "@types/node": "^18.7.23",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "esbuild": "^0.15.10",
    "eslint": "^8.24.0",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.15",
    "typescript": "^4.8.4"
  },
  "devDependencies": {
    "@types/jest": "^29.1.0",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "none",
    "semi": false
  },
  "bugs": {
    "url": "https://github.com/Internet-Computer-Tools/seekr/issues"
  },
  "homepage": "https://github.com/Internet-Computer-Tools/seekr#readme",
  "main": "dist/tsc/src/index.js",
  "directories": {
    "test": "test"
  }
}
